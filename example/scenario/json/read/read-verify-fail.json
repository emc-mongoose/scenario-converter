{
	"type": "sequential",
	"config": {
		"item": {
			"output": {
				"path": "read-verify-fail"
			},
			"data": {
				"size": "1KB"
			}
		}
	},
	"steps": [
		{
			"type": "precondition",
			"config": {
				"item": {
					"output": {
						"file": "read-verify-fail.csv"
					}
				},
				"test": {
					"step": {
						"limit": {
							"count": 100
						}
					}
				}
			}
		}, {
			"type": "precondition",
			"config": {
				"item": {
					"data": {
						"ranges": {
							"random": 1
						}
					},
					"input": {
						"file": "read-verify-fail.csv"
					}
				}
			}
		}, {
			"type": "load",
			"config": {
				"item": {
					"data": {
						"verify": true
					},
					"input": {
						"file": "read-verify-fail.csv"
					}
				},
				"load": {
					"type": "read"
				}
			}
		}
	]
}

