{
	"type": "sequential",
	"steps": [
		{
			"type": "precondition",
			"config": {
				"item": {
					"output": {
						"file": "circular-read.csv"
					}
				},
				"test": {
					"step": {
						"limit": {
							"count": 1
						}
					}
				}
			}
		}, {
			"type": "load",
			"config": {
				"item": {
					"input": {
						"file": "circular-read.csv"
					}
				},
				"load": {
					"type": "read",
					"generator": {
						"recycle": {
							"enabled": true
						}
					}
				},
				"test": {
					"step": {
						"limit": {
							"time": "1m"
						}
					}
				}
			}
		}
	]
}
