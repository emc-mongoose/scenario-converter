{
  "type": "sequential",
  "steps": [
    {
      "type": "command",
      "value": "rm $ITEM_INPUT_FILE"
    },
    {
      "type": "command",
      "value": "export ITEM_INPUT_FILE=10K_items.csv"
    },
    {
      "type": "precondition",
      "config": {
        "item": {
          "output": {
            "file": "{$ITEM_INPUT_FILE}"
          }
        },
        "test": {
          "step": {
            "limit": {
              "count": 10000
            }
          }
        }
      }
    },
    {
      "type": "parallel",
      "steps": [
        {
          "type": "load",
          "config": {
            "item": {
              "input": {
                "file": "{$ITEM_INPUT_FILE}"
              }
            },
            "load": {
              "generator": {
                "recycle": {
                  "enabled": true
                }
              },
              "type": "read"
            }
          }
        },
        {
          "type": "for",
          "value": "itemSize",
          "in": [
            "10KB",
            "1MB",
            "100MB"
          ],
          "steps": [
            {
              "type": "command",
              "value": "echo ${itemSize}"
            },
            {
              "type": "for",
              "value": "concurrencyLimit",
              "in": [
                1,
                10,
                100
              ],
              "steps": [
                {
                  "type": "command",
                  "value": "echo ${concurrencyLimit}"
                },
                {
                  "type": "load",
                  "config": {
                    "item": {
                      "data": {
                        "size": "${itemSize}"
                      }
                    },
                    "load": {
                      "limit": {
                        "concurrency": "${concurrencyLimit}"
                      }
                    },
                    "test": {
                      "step": {
                        "limit": {
                          "time": "1m"
                        }
                      }
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
