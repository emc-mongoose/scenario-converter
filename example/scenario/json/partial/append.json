{
	"type": "sequential",
	"config": {
		"item": {
			"data": {
				"size": "8KB"
			},
			"output": {
				"path": "partial-append"
			}
		},
		"test": {
			"step": {
				"limit": {
					"count": 100
				}
			}
		}
	},
	"steps": [
		{
			"type": "precondition",
			"config": {
				"item": {
					"output": {
						"file": "partial-append-0.csv"
					}
				}
			}
		}, {
			"type": "load",
			"config": {
				"item": {
					"data": {
						"ranges": {
							"fixed": [
								"-8KB-"
							]
						}
					},
					"input": {
						"file": "partial-append-0.csv"
					},
					"output": {
						"file": "partial-append-1.csv"
					}
				},
				"load": {
					"type": "update"
				}
			}
		}, {
			"type": "load",
			"config": {
				"item": {
					"data": {
						"verify": true
					},
					"input": {
						"file": "partial-append-1.csv"
					}
				},
				"load": {
					"type": "read"
				}
			}
		}
	]
}
