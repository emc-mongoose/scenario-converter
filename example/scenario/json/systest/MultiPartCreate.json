{
	"type": "sequential",
	"steps": [
		{
			"type": "command",
			"value": "rm -f ${ITEM_OUTPUT_FILE}"
		},
		{
			"type": "load",
			"config": {
				"item": {
					"data": {
						"ranges": {
							"threshold": "${PART_SIZE}"
						}
					},
					"output": {
						"file": "${ITEM_OUTPUT_FILE}"
					}
				},
				"load": {
					"batch": {
						"size": 1
					}
				},
				"storage": {
					"net": {
						"http": {
							"namespace": "ns1"
						}
					}
				},
				"test": {
					"step": {
						"limit": {
							"size": "${SIZE_LIMIT}"
						}
					}
				}
			}
		}
	]
}
