{
	"type": "sequential",
	"steps": [
		{
			"type": "command",
			"value": "rm -f weighted-load.csv"
		},
		{
			"type": "precondition",
			"config": {
				"item": {
					"output": {
						"file": "weighted-load.csv"
					}
				},
				"test": {
					"step": {
						"limit": {
							"count": 10000,
							"size": "1GB",
							"time": 10
						}
					}
				}
			}
		},
		{
			"type": "mixed",
			"weights": [
				20, 80
			],
			"config": [
				{
					"item": {
						"output": {
							"path": "${ITEM_OUTPUT_PATH}"
						}
					},
					"test": {
						"step": {
							"limit": {
								"time": 50
							}
						}
					}
				},
				{
					"item": {
						"input": {
							"file": "weighted-load.csv"
						}
					},
					"load": {
						"generator": {
							"recycle": {
								"enabled": true
							}
						},
						"type": "read"
					}
				}
			]
		}
	]
}
