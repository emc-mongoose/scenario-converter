{
	"type": "sequential",
	"config": {
		"storage": {
			"net": {
				"http": {
					"namespace": "ns1"
				}
			}
		}
	},
	"steps": [
		{
			"type": "command",
			"value": "rm -f HttpStorageMetricsThresholdTest.csv"
		},
		{
			"type": "load",
			"config": {
				"item": {
					"output": {
						"file": "HttpStorageMetricsThresholdTest.csv",
						"path": "/default"
					}
				},
				"output": {
					"metrics": {
						"threshold": 0.8
					}
				},
				"test": {
					"step": {
						"limit": {
							"count": 500000,
							"time": "30s"
						}
					}
				}
			}
		},
		{
			"type": "load",
			"config": {
				"item": {
					"input": {
						"file": "HttpStorageMetricsThresholdTest.csv"
					}
				},
				"output": {
					"metrics": {
						"threshold": 0.8
					}
				},
				"load": {
					"type": "read"
				}
			}
		},
		{
			"type": "load",
			"config": {
				"item": {
					"data": {
						"ranges": {
							"random": 10
						}
					},
					"input": {
						"file": "HttpStorageMetricsThresholdTest.csv"
					}
				},
				"output": {
					"metrics": {
						"threshold": 0.8
					}
				},
				"load": {
					"type": "update"
				}
			}
		},
		{
			"type": "precondition",
			"config": {
				"item": {
					"input": {
						"file": "HttpStorageMetricsThresholdTest.csv"
					}
				},
				"load": {
					"type": "delete",
					"limit": {
						"concurrency": 50
					}
				},
				"output": {
					"metrics": {
						"average": {
							"persist": false
						},
						"summary": {
							"persist": false
						},
						"trace": {
							"persist": false
						}
					}
				}
			}
		}
	]
}
