{
	"type": "sequential",
	"config": {
		"item": {
			"data": {
				"size": "64KB"
			}
		},
		"test": {
			"step": {
				"limit": {
					"count": 1
				}
			}
		},
		"storage": {
			"driver": {
				"type": "fs"
			}
		}
	},
	"steps": [
		{
			"type": "precondition",
			"config": {
				"item": {
					"output": {
						"file": "circular-file-overwrite.csv",
						"path": "circular-file-overwrite"
					}
				}
			}
		}, {
			"type": "load",
			"config": {
				"item": {
					"input": {
						"file": "circular-file-overwrite.csv"
					}
				},
				"load": {
					"type": "update",
					"generator": {
						"recycle": {
							"enabled": true
						}
					}
				},
				"test": {
					"step": {
						"limit": {
							// count -> infinity instead of inherited value, time -> 5 minutes
							"count": 0,
							"time": "5m"
						}
					}
				}
			}
		}
	]
}
