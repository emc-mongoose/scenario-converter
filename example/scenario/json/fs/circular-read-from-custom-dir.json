{
	"type": "sequential",
	"config": {
		"item": {
			"output": {
				"path": "fs-circular-read-from-custom-dir"
			}
		},
		"test": {
			"step": {
				"limit": {
					"count": 10
				}
			}
		},
		"storage": {
			"driver": {
				"type": "fs"
			}
		}
	},
	"steps": [
		{
			"type": "precondition"
		}, {
			"type": "load",
			"config": {
				"item": {
					"input": {
						"path": "fs-circular-read-from-custom-dir"
					}
				},
				"load": {
					"type": "read",
					"generator": {
						"recycle": {
							"enabled": true
						}
					}
				},
				"test": {
					"step": {
						"limit": {
							// count -> infinity, time -> 1 minute
							"count": 0,
							"time": "1m"
						}
					}
				}
			}
		}
	]
}

