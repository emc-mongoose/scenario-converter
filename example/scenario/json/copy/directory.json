{
	"type": "sequential",
	"config": {
		"item": {
			"output": {
				"path": "copy-directory"
			},
			"data": {
				"size": "10KB"
			},
			"type": "path"
		},
		"storage": {
			"driver": {
				"type": "fs"
			}
		}
	},
	"steps": [
		{
			"type": "precondition",
			"config": {
				"item": {
					"output": {
						"file": "copy-directory.json"
					},
					"naming": {
						"type": "asc",
						"length": 1
					}
				},
				"test": {
					"step": {
						"limit": {
							"count": 1
						}
					}
				}
			}
		}, {
			"type": "precondition",
			"config": {
				"item": {
					"output": {
						"path": "copy-directory/1"
					},
					"type": "data"
				},
				"load": {
					"limit": {
						"concurrency": 10
					}
				},
				"test": {
					"step": {
						"limit": {
							"count": 100
						}
					}
				}
			}
		}, {
			"type": "load",
			"config": {
				"item": {
					"output": {
						"path": "copy-directory_"
					},
					"input": {
						"path": "copy-directory",
						"file": "copy-directory.json"
					}
				},
				"load": {
					"limit": {
						"concurrency": 5
					}
				},
				"test": {
					"step": {
						"limit": {
							"count": 100
						}
					}
				}
			}
		}
	]
}
